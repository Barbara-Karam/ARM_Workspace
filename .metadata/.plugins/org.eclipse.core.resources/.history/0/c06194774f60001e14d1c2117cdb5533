#ifndef AFIO_PRIVATE_H_
#define AFIO_PRIVATE_H_

typedef struct{
	u32		AFIO_EVCR;
	u32		AFIO_MAPR;
	u32		AFIO_EXTICR1;
	u32		AFIO_EXTICR2;
	u32		AFIO_EXTICR3;
	u32		AFIO_EXTICR4;
	u32		AFIO_MAPR2;
}AFIO_reg;

#define AFIO ((AFIO_reg*)0x40010000)

#endif /* AFIO_PRIVATE_H_ */
